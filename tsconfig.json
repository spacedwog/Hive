{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-native",
    "module": "node16",
    "target": "esnext",
    "moduleResolution": "node16",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "skipLibCheck": true,

    "typeRoots": [
      "./src/types",
      "./node_modules/@types"
    ],

    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "app",
    "env.d.ts",
    "victory-native.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "api/lib/sensor/SensorApiHandler.js"
, "api/firewall.js"  ]
}